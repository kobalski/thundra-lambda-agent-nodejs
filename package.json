{
  "name": "@thundra/core",
  "version": "1.0.1",
  "description": "Thundra Lambda Node.js Agent",
  "main": "dist/thundra.js",
  "scripts": {
    "build": "webpack",
    "start": "npm-run-all --parallel watch:server watch:build",
    "test": "jest --coverage",
    "test-ci": "jest --config config.json --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "watch:build": "webpack --watch",
    "watch:server": "nodemon --inspect=\"5858\" \"./build/bundle.js\" --watch \"./build\" "
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thundra-io/thundra-lambda-agent-nodejs.git"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "serverless",
    "aws",
    "lambda",
    "observability",
    "tracing",
    "metrics",
    "analytics",
    "telemetry"
  ],
  "author": "Thundra <info@thundra.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "i": "^0.3.6",
    "jest": "^22.4.2",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "uuid": "^3.2.1"
  }
}
